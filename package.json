{
  "name": "scuffle",
  "version": "0.2.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "check-word": "^1.1.0",
    "concurrently": "^5.2.0",
    "crypto": "^1.0.1",
    "node-sass": "^7.0.0",
    "path": "^0.12.7",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "sweetalert2": "^9.17.1",
    "sweetalert2-react-content": "^3.0.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1"
  },
  "scripts": {
    "server": "nodemon server/index.js",
    "client": "nodemon server/index.js",
    "dev": "concurrently \"react-scripts start\" \"yarn run client\"",
    "start": "node server/index.js",
    "lint": "eslint --fix --ignore-path .gitignore .",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false yarn install --prefix client && yarn run build --prefix client"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
